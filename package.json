{
  "name": "bliskavka-com",
  "version": "0.0.0",
  "private": true,
  "hexo": {
    "version": "7.1.1"
  },
  "scripts": {
    "build": "rimraf public && hexo generate",
    "cdk": "cdk",
    "synth": "npm run cdk -- synth",
    "deploy": "npm run cdk -- deploy --require-approval never",
    "diff": "npm run cdk -- diff",
    "start": "hexo server -p 3000 -o",
    "publish": "npm run build && aws s3 sync ./public s3://bliskavka-com-assets --delete --region us-east-1 --profile lsw && aws cloudfront create-invalidation --distribution-id EKNPBAEUWPOEB --paths '/*' --region us-east-1 --profile lsw"
  },
  "dependencies": {
    "@aws/pdk": "^0.23.46",
    "@types/node": "^20.14.10",
    "aws-cdk-lib": "^2.147.0",
    "hexo": "^7.1.1",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^3.0.0",
    "hexo-generator-json-content": "^4.2.3",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.2.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0"
  },
  "devDependencies": {
    "aws-cdk": "^2.147.0",
    "rimraf": "^6.0.0",
    "ts-node": "^10.9.2"
  }
}